ourscripts:Toggle("Esp",false,function(val)
  getgenv().Values.ESP = val
  if getgenv().Values.ESP == true then
  for i , v in pairs(game.Players:GetChildren()) do 
      if not v.Character:FindFirstChild("Head"):FindFirstChild("BillboardGui") then
      local BillboardGui = Instance.new("BillboardGui")
      local TextLabel = Instance.new("TextLabel")
      
      BillboardGui.ResetOnSpawn = false
      BillboardGui.Parent = v.Character.Head
      BillboardGui.ZIndexBehavior = Enum.ZIndexBehavior.Global
      BillboardGui.Active = true
      BillboardGui.AlwaysOnTop = true
      BillboardGui.LightInfluence = 1.000
      BillboardGui.Size = UDim2.new(4 ,0, 1, 0)
      BillboardGui.StudsOffset = Vector3.new(0, 2, 0)
      
      TextLabel.Parent = BillboardGui
      TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
      TextLabel.BackgroundTransparency = 1.000
      TextLabel.Size = UDim2.new(1, 0, 1, 0)
      TextLabel.Font = Enum.Font.TitilliumWeb
      TextLabel.Text = v.DisplayName .."(@" .. v.Name..")"
      TextLabel.TextColor3 = Color3.fromRGB(51, 255, 58)
      TextLabel.TextSize = 20
      TextLabel.TextWrapped = false
      else
          return
      end
  end
else
  for i , v in pairs(game.Players:GetChildren()) do
      if v.Character:FindFirstChild("Head"):FindFirstChild("BillboardGui")then
      v.Character:FindFirstChild("Head"):FindFirstChild("BillboardGui"):Destroy()
      end
  end
end;

end)

