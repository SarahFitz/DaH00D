ourscripts:Button("Anti-Bag", function(f)
while true do
    wait()
    for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if v:IsA("MeshPart") and v.Name == "Christmas_Sock" then
            v:Destroy()
        end
    end

    game.Players.LocalPlayer.Character.ChildAdded:Connect(
        function(sock)
            if sock:IsA("MeshPart") and sock.Name == "Christmas_Sock" then
                sock:Destroy()
            end
        end
    )
  end
end);

